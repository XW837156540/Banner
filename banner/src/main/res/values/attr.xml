<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="xw_Banner">
        <attr name="xw_delay_time" format="integer" />
        <attr name="xw_scroll_time" format="integer" />
        <attr name="xw_is_auto_play" format="boolean" />
        <attr name="xw_title_background" format="color|reference" />
        <attr name="xw_title_textcolor" format="color" />
        <attr name="xw_title_textsize" format="dimension" />
        <attr name="xw_title_height" format="dimension" />
        <attr name="xw_indicator_width" format="dimension" />
        <attr name="xw_indicator_height" format="dimension" />
        <attr name="xw_indicator_margin" format="dimension" />
        <attr name="xw_indicator_drawable_selected" format="reference" />
        <attr name="xw_indicator_drawable_unselected" format="reference" />
        <attr name="xw_banner_layout" format="reference"/>
        <attr name="xw_banner_default_image" format="reference"/>
        <attr name="xw_image_scale_type" format="enum">
            <enum name="xw_center" value="0" />
            <enum name="xw_center_crop" value="1" />
            <enum name="xw_center_inside" value="2" />
            <enum name="xw_fit_center" value="3" />
            <enum name="xw_fit_end" value="4" />
            <enum name="xw_fit_start" value="5" />
            <enum name="xw_fit_xy" value="6" />
            <enum name="xw_matrix" value="7" />
        </attr>
        <!--<attr name="indicator_animator" format="reference" />-->
        <!--<attr name="indicator_animator_reverse" format="reference" />-->
    </declare-styleable>
</resources>